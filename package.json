{
  "name": "doraeeemon",
  "private": true,
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "lint": "biome lint src --no-errors-on-unmatched",
    "fmt": "biome format src --write --no-errors-on-unmatched",
    "check": "biome check --write --no-errors-on-unmatched",
    "dev": "concurrently \"tsc --watch\" \"nodemon --watch src/ --ext ts,tsx --exec ts-node -r tsconfig-paths/register --files --transpile-only src/index.ts\"",
    "typecheck": "tsc --noEmit",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@lazidog/utils": "^1.0.1",
    "mezon-sdk": "^2.8.11",
    "minimatch": "^10.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/node": "^24.3.0",
    "concurrently": "^8.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}